/**
 * 4-way demultiplexor:
 * [a, b, c, d] = [in, 0, 0, 0] if sel = 00
 *                [0, in, 0, 0] if sel = 01
 *                [0, 0, in, 0] if sel = 10
 *                [0, 0, 0, in] if sel = 11
 */
CHIP DMux4Way {
    IN in, sel[2];
    OUT a, b, c, d;

    PARTS:
    Dmux(in=in, sel=sel[1], a=DmuxA1, b=DmuxB1);
    Dmux(in=DmuxA1, sel=sel[0], a=a, b=b);
    Dmux(in=DmuxB1, sel=sel[0], a=c, b=d);
}
